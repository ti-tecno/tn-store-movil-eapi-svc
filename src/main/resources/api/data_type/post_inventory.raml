#%RAML 1.0 DataType
type: object
properties:
  "product":
    type: string
    required: true
    example: "zapato1"
  "amount":
    type: number
    required: true
    example: 50
  "purchase_price":
    type: number
    required: true
    example: 10
  "total_quantity":
    type: number
    required: true
    example: 500
  "product_type":
    type: string
    required: true
    example: "abarratos"